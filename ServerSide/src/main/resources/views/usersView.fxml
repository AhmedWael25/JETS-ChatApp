<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import com.jfoenix.controls.JFXTreeTableColumn?>
<?import com.jfoenix.controls.JFXTreeTableView?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<AnchorPane xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jets.chatserver.gui.controllers.UsersController">
   <children>
      <StackPane fx:id="root" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
          <VBox prefHeight="500.0" prefWidth="800.0" spacing="10">
              <children>
               <HBox alignment="CENTER">
                  <children>
                     <Label styleClass="title-label" text="DataBase Users" />
                     <Label fx:id="treeTableViewCount" alignment="CENTER_RIGHT" minWidth="80" styleClass="title-label" textAlignment="RIGHT" />
                     <StackPane HBox.hgrow="ALWAYS">
                        <children>
                           <StackPane maxWidth="200" StackPane.alignment="CENTER_RIGHT">
                              <children>
                                 <JFXTextField fx:id="searchField" promptText="Search..." unFocusColor="#4059a9" />
                              </children>
                           </StackPane>
                        </children>
                     </StackPane>
                  </children>
               </HBox>
                  <JFXTreeTableView fx:id="treeTableView">
                      <columns>
                          <JFXTreeTableColumn fx:id="phoneEditableColumn" minWidth="133.0" prefWidth="120.0" resizable="false" text="Phone" />
                          <JFXTreeTableColumn fx:id="nameEditableColumn" minWidth="133" prefWidth="120.0" resizable="false" text="Name" />
                          <JFXTreeTableColumn fx:id="genderEditableColumn" minWidth="133" prefWidth="120.0" resizable="false" text="Gender" />
                     <JFXTreeTableColumn fx:id="emailEditableColumn" minWidth="133" prefWidth="120.0" resizable="false" text="Email" />
                     <JFXTreeTableColumn fx:id="countryEditableColumn" minWidth="133" prefWidth="120.0" text="Country" />
                      </columns>
                  </JFXTreeTableView>
               <HBox prefHeight="61.0" prefWidth="718.0">
                  <children>
                     <GridPane prefHeight="140.0" prefWidth="602.0">
                       <columnConstraints>
                         <ColumnConstraints hgrow="SOMETIMES" maxWidth="96.0" minWidth="10.0" prefWidth="30.0" />
                         <ColumnConstraints hgrow="SOMETIMES" maxWidth="216.0" minWidth="0.0" prefWidth="216.0" />
                           <ColumnConstraints hgrow="SOMETIMES" maxWidth="231.0" minWidth="0.0" prefWidth="18.0" />
                           <ColumnConstraints hgrow="SOMETIMES" maxWidth="231.0" minWidth="10.0" prefWidth="128.0" />
                           <ColumnConstraints hgrow="SOMETIMES" maxWidth="300.0" minWidth="10.0" prefWidth="210.0" />
                       </columnConstraints>
                       <rowConstraints>
                           <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                           <RowConstraints maxHeight="16.0" minHeight="10.0" prefHeight="16.0" vgrow="SOMETIMES" />
                           <RowConstraints maxHeight="9.0" minHeight="0.0" prefHeight="0.0" vgrow="SOMETIMES" />
                           <RowConstraints maxHeight="14.0" minHeight="10.0" prefHeight="13.0" vgrow="SOMETIMES" />
                           <RowConstraints maxHeight="10.0" minHeight="10.0" prefHeight="10.0" vgrow="SOMETIMES" />
                           <RowConstraints maxHeight="10.0" minHeight="10.0" prefHeight="10.0" vgrow="SOMETIMES" />
                           <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                       </rowConstraints>
                        <children>
                           <JFXTextField fx:id="userPhoneNum" onAction="#addUser" alignment="CENTER" prefHeight="25.0" prefWidth="208.0" promptText="Phone Number" unFocusColor="#4059a9" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                           <FontIcon fx:id="ftPhone" iconColor="#4059a9" iconLiteral="fas-phone" textAlignment="RIGHT" wrappingWidth="75.0" GridPane.rowIndex="1" />
                           <JFXButton alignment="BASELINE_CENTER" onAction="#addUser" prefHeight="25.0" prefWidth="108.0" style="-fx-background-color: #4059a9;" text="Add User" GridPane.columnIndex="3" GridPane.rowIndex="3" />
                           <Label fx:id="registerLb" prefHeight="17.0" prefWidth="220.0" textFill="#d73838" GridPane.columnIndex="4" GridPane.rowIndex="3" />
                        </children>
                     </GridPane>
                  </children>
               </HBox>
              </children>
          </VBox>
      </StackPane>
   </children>
</AnchorPane>
