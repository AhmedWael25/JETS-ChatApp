<AnchorPane xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jets.chatserver.gui.controllers.UsersController">
   <children>
      <StackPane fx:id="root" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
          <VBox prefHeight="500.0" prefWidth="800.0" spacing="10">
              <children>
               <HBox alignment="CENTER">
                  <children>
                     <Label styleClass="title-label" text="DataBase Users" />
                     <Label fx:id="treeTableViewCount" alignment="CENTER_RIGHT" minWidth="80" styleClass="title-label" textAlignment="RIGHT" />
                     <StackPane HBox.hgrow="ALWAYS">
                        <children>
                           <StackPane maxWidth="200" StackPane.alignment="CENTER_RIGHT">
                              <children>
                                 <JFXTextField fx:id="searchField" promptText="Search..." />
                              </children>
                           </StackPane>
                        </children>
                     </StackPane>
                  </children>
               </HBox>
                  <JFXTreeTableView fx:id="treeTableView">
                      <columns>
                          <JFXTreeTableColumn fx:id="phoneEditableColumn" minWidth="133.0" prefWidth="120.0" resizable="false" text="Phone" />
                          <JFXTreeTableColumn fx:id="nameEditableColumn" minWidth="133" prefWidth="120.0" resizable="false" text="Name" />
                          <JFXTreeTableColumn fx:id="genderEditableColumn" minWidth="133" prefWidth="120.0" resizable="false" text="Gender" />
                     <JFXTreeTableColumn fx:id="emailEditableColumn" minWidth="133" prefWidth="120.0" resizable="false" text="Email" />
                     <JFXTreeTableColumn fx:id="countryEditableColumn" minWidth="133" prefWidth="120.0" text="Country" />
                      </columns>
                  </JFXTreeTableView>
               <HBox prefHeight="61.0" prefWidth="718.0">
                  <children>
                     <GridPane prefHeight="97.0" prefWidth="491.0">
                       <columnConstraints>
                         <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                         <ColumnConstraints hgrow="SOMETIMES" maxWidth="197.0" minWidth="10.0" prefWidth="191.0" />
                           <ColumnConstraints hgrow="SOMETIMES" maxWidth="231.0" minWidth="10.0" prefWidth="74.0" />
                           <ColumnConstraints hgrow="SOMETIMES" maxWidth="231.0" minWidth="10.0" prefWidth="189.0" />
                       </columnConstraints>
                       <rowConstraints>
                         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                           <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                           <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                           <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                           <RowConstraints maxHeight="16.0" minHeight="10.0" prefHeight="16.0" vgrow="SOMETIMES" />
                           <RowConstraints maxHeight="9.0" minHeight="0.0" prefHeight="0.0" vgrow="SOMETIMES" />
                           <RowConstraints maxHeight="14.0" minHeight="10.0" prefHeight="13.0" vgrow="SOMETIMES" />
                           <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                           <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                           <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                           <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                           <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                           <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                       </rowConstraints>
                        <children>
                           <JFXTextField fx:id="userPhoneNum" alignment="CENTER" prefHeight="25.0" prefWidth="123.0" promptText="User Phone Number" GridPane.columnIndex="1" GridPane.rowIndex="6" />
                           <FontIcon fx:id="ftPhone" iconLiteral="fas-phone" wrappingWidth="42.0" GridPane.columnIndex="1" GridPane.rowIndex="6" />
                           <JFXButton alignment="BASELINE_CENTER" onAction="#addUser" prefHeight="25.0" prefWidth="135.0" style="-fx-background-color: #4059a9;" text="Add User" textFill="#f8f8f8" GridPane.columnIndex="3" GridPane.rowIndex="8" />
                        </children>
                     </GridPane>
                  </children>
               </HBox>
              </children>
          </VBox>
      </StackPane>
   </children>
</AnchorPane>
